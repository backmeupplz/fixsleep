    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "pluginSearchDirs": [
      "."
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
